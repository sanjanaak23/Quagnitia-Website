{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.js$)",
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.css$)",
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.png$|\\.jpg$|\\.jpeg$|\\.gif$|\\.svg$|\\.ico$|\\.webp$)",
      "dest": "/$1"
    },
    {
      "src": "/admin/blog",
      "dest": "/index.html"
    },
    {
      "src": "/blog/.*",
      "dest": "/index.html"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}